{
  "name": "danfe-extractor",
  "version": "1.0.0",
  "description": "A simple script to extract danfe pdf values",
  "main": "index.js",
  "scripts": {
    "process": "npm run clear:output && npm run extract && node ./src/scripts/process.js",
    "extract": "node ./src/scripts/extract.js",
    "read:temp": "node ./src/scripts/read-temp.js",
    "read:result": "node ./src/scripts/read-result.js",
    "clear:output": "node ./src/scripts/clear-output.js",
    "clear:input": "node ./src/scripts/clear-input.js"
  },
  "keywords": [
    "danfe",
    "pdf",
    "extractor",
    "script"
  ],
  "author": "marcelo serra",
  "license": "ISC",
  "dependencies": {
    "pdf2json": "^3.1.1"
  },
  "type": "module"
}